@model IEnumerable<UI.Models.ReferenciaViewModel>

@{
  ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
  @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
  <tr>
    <th>
      @Html.DisplayNameFor(model => model.LinhaId)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.AtendimentoId)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.Sentido)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.PInicioId)
    </th>
    <th>
      @Html.DisplayNameFor(model => model.PTerminoId)
    </th>
    <th></th>
  </tr>

  @foreach (var item in Model) {
    <tr>
      <td>
        @Html.DisplayFor(modelItem => item.LinhaId)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.AtendimentoId)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.Sentido)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.PInicioId)
      </td>
      <td>
        @Html.DisplayFor(modelItem => item.PTerminoId)
      </td>
      <td>
        @Html.ActionLink("Edit", "Edit", new { id = item.Id }) |
        @Html.ActionLink("Details", "Details", new { id = item.Id }) |
        @Html.ActionLink("Delete", "Delete", new { id = item.Id })
      </td>
    </tr>
  }

</table>
